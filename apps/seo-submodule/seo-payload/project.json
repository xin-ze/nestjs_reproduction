{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/seo-submodule/seo-payload",
  "projectType": "application",
  "targets": {
    "build-admin-panel": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["PAYLOAD_CONFIG_PATH=apps/seo-submodule/seo-payload/src/payload.config.ts payload build "],
        "parallel": false
      },
      "configurations": {
        "stage": {
          "commands": [
            "cp apps/seo-submodule/seo-payload/src/environments/environment.stage.ts apps/seo-submodule/seo-payload/src/environments/environment.ts",
            "PAYLOAD_CONFIG_PATH=apps/seo-submodule/seo-payload/src/payload.config.ts payload build"
          ]
        },
        "prod": {
          "commands": [
            "cp apps/seo-submodule/seo-payload/src/environments/environment.prod.ts apps/seo-submodule/seo-payload/src/environments/environment.ts",
            "PAYLOAD_CONFIG_PATH=apps/seo-submodule/seo-payload/src/payload.config.ts payload build"
          ]
        }
      }
    },
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/seo-submodule/seo-payload",
        "main": "apps/seo-submodule/seo-payload/src/main.ts",
        "tsConfig": "apps/seo-submodule/seo-payload/tsconfig.app.json",
        "assets": ["apps/seo-submodule/seo-payload/src/assets", "apps/seo-submodule/seo-payload/node_modules"],
        "webpackConfig": "apps/seo-submodule/seo-payload/webpack.config.js",
        "envFile": "apps/seo-submodule/seo-payload/.env"
      },
      "configurations": {
        "e2e": {
          "fileReplacements": [
            {
              "replace": "apps/seo-submodule/seo-payload/src/environments/environment.ts",
              "with": "apps/seo-submodule/seo-payload/src/environments/environment.e2e.ts"
            }
          ]
        },
        "stage": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/seo-submodule/seo-payload/src/environments/environment.ts",
              "with": "apps/seo-submodule/seo-payload/src/environments/environment.stage.ts"
            }
          ],
          "envFile": "apps/seo-submodule/seo-payload/.env.production"
        },
        "prod": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/seo-submodule/seo-payload/src/environments/environment.ts",
              "with": "apps/seo-submodule/seo-payload/src/environments/environment.prod.ts"
            }
          ],
          "envFile": "apps/seo-submodule/seo-payload/.env.production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "seo-payload:build"
      },
      "configurations": {
        "e2e": {
          "buildTarget": "seo-payload:build:e2e"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/seo-submodule/seo-payload/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/seo-submodule/seo-payload"],
      "options": {
        "jestConfig": "apps/seo-submodule/seo-payload/jest.config.ts",
        "passWithNoTests": true,
        "outputFile": "tmp/seo-payload.output.json"
      }
    },
    "ts-codegen": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "nx run payload-types:ts-codegen"
      }
    },
    "db-migration-status": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/seo-submodule/seo-payload",
        "commands": ["ts-node src/migrations status"],
        "parallel": false
      },
      "configurations": {
        "stage": {
          "commands": ["ENV=stage ts-node src/migrations status"]
        },
        "prod": {
          "commands": ["ENV=prod ts-node src/migrations status"]
        }
      }
    },
    "db-migration-up": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/seo-submodule/seo-payload",
        "commands": ["ts-node src/migrations up"],
        "parallel": false
      },
      "configurations": {
        "stage": {
          "commands": ["ENV=stage ts-node src/migrations up"]
        },
        "prod": {
          "commands": ["ENV=prod ts-node src/migrations up"]
        }
      }
    },
    "db-migration-down": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/seo-submodule/seo-payload",
        "commands": ["ts-node src/migrations down -l"],
        "parallel": false
      },
      "configurations": {
        "stage": {
          "commands": ["ENV=stage ts-node src/migrations down -l"]
        },
        "prod": {
          "commands": ["ENV=prod ts-node src/migrations down -l"]
        }
      }
    },
    "db-migration-new": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["ts-node apps/seo-submodule/seo-payload/src/migrations new -n {args.file}"],
        "parallel": false
      }
    },
    "run-script": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["apps/seo-submodule/seo-payload/scripts/run-payload-script.sh {args.file}"],
        "parallel": false
      },
      "configurations": {
        "stage": {
          "commands": ["ENV=stage nx run seo-payload:run-script --file={args.file}"]
        },
        "prod": {
          "commands": ["ENV=prod nx run seo-payload:run-script --file={args.file}"]
        }
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/seo-submodule/seo-payload/scripts",
        "commands": ["./deploy-image", "./deploy-stack"],
        "parallel": false
      }
    },
    "check-ts": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["npm run check-ts seo-payload"],
        "parallel": false
      }
    }
  },
  "tags": []
}
